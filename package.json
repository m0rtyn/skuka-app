{
  "name": "skuka-app",
  "private": true,
  "version": "0.10.3",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "npm run start",
    "build": "vite build && npm run copy-changelog",
    "serve": "vite preview",
    "format": "vite format",
    "test": "vitest",
    "test:debug": "vitest --debug",
    "coverage": "vitest run --coverage",
    "build-functions": "cd functions && npm install && npm run build",
    "new-version": "standard-version",
    "bump": "npm run new-version",
    "analyze": "source-map-explorer 'dist/static/js/*.js'",
    "copy-changelog": "cp CHANGELOG.md public/CHANGELOG.txt",
    "inject-app-version": "export VITE_VERSION=$npm_package_version && VITE_NAME=$npm_package_name && echo \"VITE_VERSION: $VITE_VERSION $npm_package_version\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.8.2",
    "date-fns": "^2.25.0",
    "firebase": "^11.9.1",
    "nosleep.js": "^0.12.0",
    "rc-progress": "^3.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.6.2",
    "react-swipeable": "^7.0.2",
    "react-tooltip": "^5.28.0",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.19",
    "use-long-press": "^3.1.3",
    "use-sound": "^5.0.0",
    "web-vitals": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/date-fns": "^2.6.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-config-code-complexity": "^1.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^3.3.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "source-map-explorer": "^2.5.2",
    "standard-version": "^9.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}